<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.lurenman.eventhandlingdemo.MainActivity">
    <com.example.lurenman.eventhandlingdemo.views.MyLinearLayoutView
        android:id="@+id/ml_LinearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">
        <com.example.lurenman.eventhandlingdemo.views.EventButton
            android:id="@+id/eb_EventButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="EventButton"
            />
    </com.example.lurenman.eventhandlingdemo.views.MyLinearLayoutView>



</LinearLayout>
